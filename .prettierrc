{
  "endOfLine": "lf",
  "semi": false,
  "tabWidth": 2,
  "printWidth": 300,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "trailingComma": "none",
  "insertPragma": false,
  "jsxBracketSameLine": false,
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "requirePragma": false,
  "arrowParens": "always",
  "bracketSpacing": true,
}
